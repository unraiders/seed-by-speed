<?xml version="1.0"?>
<Container>
  <Name>seed-by-speed</Name>
  <Repository>unraiders/seed-by-speed</Repository>
  <Registry>https://hub.docker.com/r/unraiders/seed-by-speed/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/unraiders/seed-by-speed</Support>
  <Icon>https://raw.githubusercontent.com/unraiders/imagenes/master/seed-by-speed.png</Icon>
  <Overview>
    Pausa/Reanuda torrents en qBittorrent basado en la velocidad de upload fuera de tiempo de sedeo.
  </Overview>
  <Category>Tools:</Category>
  <Config Name="QBITTORRENT_HOST" Target="QBITTORRENT_HOST" Default="" Mode="" Description="Host de qBittorrent, ejemplo: 192.168.2.20" Type="Variable" Display="always" Required="true"></Config>
  <Config Name="QBITTORRENT_PORT" Target="QBITTORRENT_PORT" Default="" Mode="" Description="Puerto de qBittorrent, ejemplo: 8090" Type="Variable" Display="always" Required="true"></Config>
  <Config Name="QBITTORRENT_USER" Target="QBITTORRENT_USER" Default="admin" Mode="" Description="Usuario de qBittorrent" Type="Variable" Display="always" Required="true"></Config>
  <Config Name="QBITTORRENT_PASSWORD" Target="QBITTORRENT_PASSWORD" Default="admin" Mode="" Description="ContraseÃ±a de qBittorrent" Type="Variable" Display="always" Required="true" Mask="true"></Config>
  <Config Name="DEBUG" Target="DEBUG" Default="0|1" Mode="" Description="Habilita el modo Debug en el log. (0 = No / 1 = Si)" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CRON_PAUSAR" Target="CRON_PAUSAR" Default="*/30 * * * *" Mode="" Description="Cron para pausar las descargas" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="CRON_REANUDAR" Target="CRON_REANUDAR" Default="0 7 * * *" Mode="" Description="Cron para reanudar las descargas" Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DATOS" Target="/app/data" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/seed-by-speed/</Config>
</Container>